<%init>
if ( $ARGSRef->{UpdateMISPEvent} ) {
    my $event_id = $Ticket->FirstCustomFieldValue('MISP Event ID');

    if ( not $event_id ) {
        push @$Actions, 'No MISP Event ID found, nothing to update';
        return;
    }

    my ($ok, $msg) = RTIR::Extension::MISP::AddRTIRObjectToMISP($Ticket);

    push @$Actions, $msg;
}
</%init>
<%args>
$Ticket
$Actions
$ARGSRef
</%args>
